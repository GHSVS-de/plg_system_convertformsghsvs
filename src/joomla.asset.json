{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "license": "{{licenseLong}}",
  "assets": [
		{
			"name": "{{name}}.override.template",
			"version": "{{version}}",
			"type": "style",
			"uri": "{{name}}.css",
			"weight": 200
		},
		{
			"name": "{{name}}.com_convertforms.overrule",
			"description": "Um die Ladereihenfolge umzudrehen, falls in ConvertForms-Optionen loadCSS aktiv. ",
			"version": "{{version}}",
			"type": "style",
			"uri": "com_convertforms/convertforms.css",
			"weight": 100
		}
  ]
}
